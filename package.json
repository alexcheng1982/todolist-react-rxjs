{
  "name": "todolist-react-rxjs",
  "version": "1.0.0",
  "description": "Todo List app - React + RxJS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "typings install",
    "start": "node server.js",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "start-dev": {
      "command": "ts-node -D ./bin/server.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "NODE_PATH": "./app",
        "PORT": 3000
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexcheng1982/todolist-react-rxjs.git"
  },
  "keywords": [
    "todo",
    "react",
    "rxjs"
  ],
  "author": "Fu Cheng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexcheng1982/todolist-react-rxjs/issues"
  },
  "homepage": "https://github.com/alexcheng1982/todolist-react-rxjs#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-polyfill": "^6.9.1",
    "better-npm-run": "0.0.10",
    "classnames": "^2.2.5",
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "typescript": "^2.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "graphql": "^0.6.2",
    "lodash.find": "^4.5.0",
    "material-ui": "^0.15.2",
    "react": "^15.2.1",
    "react-addons-linked-state-mixin": "^15.2.1",
    "react-css-modules": "^3.7.9",
    "react-dom": "^15.2.1",
    "react-flexbox-grid": "^0.10.2",
    "react-router": "^2.6.1",
    "react-tap-event-plugin": "^1.0.0",
    "rxjs": "^5.0.0-beta.10",
    "uuid": "^2.0.2",
    "webpack-isomorphic-tools": "^2.5.6"
  }
}
